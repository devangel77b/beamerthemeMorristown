\ProvidesPackage{beamerthemeMorristown}

\mode<presentation>

% requirements
\RequirePackage{tikz}

% settings
\useinnertheme{Morristown}
\useoutertheme{Morristown}
\usecolortheme{Morristown}
%\usefonttheme{professionalfonts}
%\RequirePackage{helvet}
%\RequirePackage{mathptmx}
\RequirePackage[urw-garamond]{mathdesign}
\RequirePackage[T1]{fontenc}
\RequirePackage{ebgaramond}

\setbeamertemplate{navigation symbols}{}  % no navigation on a poster

\setbeamertemplate{itemize items}[circle]

%\setbeamertemplate{bibliography item}{} % no symbols in bib
\setbeamertemplate{bibliography item}{\insertbiblabel} % numbers in bib

\setbeamertemplate{caption}{\raggedright\insertcaption\par}

\mode<all>
